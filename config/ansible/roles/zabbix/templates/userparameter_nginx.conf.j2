UserParameter=nginx.status[*],curl -s http://127.0.0.1/nginx_status | grep "^$1:" | awk '{print $$2}'
UserParameter=nginx.connections.active,curl -s http://127.0.0.1/nginx_status | grep "Active connections" | awk '{print $$3}'
UserParameter=nginx.requests.accepted,curl -s http://127.0.0.1/nginx_status | awk 'NR==3 {print $$1}'
UserParameter=nginx.requests.handled,curl -s http://127.0.0.1/nginx_status | awk 'NR==3 {print $$2}'
UserParameter=nginx.requests.total,curl -s http://127.0.0.1/nginx_status | awk 'NR==3 {print $$3}'
UserParameter=nginx.requests.reading,curl -s http://127.0.0.1/nginx_status | awk 'NR==4 {print $$2}'
UserParameter=nginx.requests.writing,curl -s http://127.0.0.1/nginx_status | awk 'NR==4 {print $$4}'
UserParameter=nginx.requests.waiting,curl -s http://127.0.0.1/nginx_status | awk 'NR==4 {print $$6}'
